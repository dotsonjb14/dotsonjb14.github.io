<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="mTest" stopProcessing="true">
                    <match url="https://.+/test/(.*)" />
                    <action type="Rewrite" url="https://localhost:8081/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
